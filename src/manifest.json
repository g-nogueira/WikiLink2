{
	"manifest_version": 2,
	"name": "WikiLink2",
	"description": "__MSG_Manifest_Description__",
	"version": "0.0.0",
	"icons": {
		"16": "Resources/Images/BrowserAction/01wikilink16.png",
		"48": "Resources/Images/BrowserAction/01wikilink48.png",
		"128": "Resources/Images/BrowserAction/01wikilink128.png"
	},
	"options_page": "Pages/Options/index.html",
	"background": {
		"scripts": [ 
			"Background/Models/BaseClass.js",
			"Background/Enums/KeyCode.js",
			"Background/Models/UserPreference.js",
			"Background/Models/UserHistory.js",
			"Background/Models/WikipediaRequest.js",
			"Background/Models/WikipediaResult.js",
			"Background/Data/Storage.js",
			"Background/API/_UserPreferences.js",
			"Background/API/_UserHistory.js",
			"Background/API/_Wikipedia.js",
			"Background/API/API.js",
			"Background/Data/Seeding.js",
			"Background/Startup.js"
		],
		"persistence": true
	},
	"default_locale": "en",
	"browser_action": {
		"default_locale": "en",
		"default_popup": "Pages/BrowserAction/index.html",
		"default_title": "__MSG_Manifest_BrowserAction_Description__"
	},
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["Pages/ContentScripts/Startup.js"],
		"css": ["Pages/ContentScripts/styles.css"],
		"run_at": "document_end"
	}],
	"permissions": [
		"storage",
		"tabs",
		"<all_urls>"
	],
	"web_accessible_resources": [
		"Resources/Images/*"
	],
	"content_security_policy": "script-src 'self' https://stackpath.bootstrapcdn.com/ https://code.jquery.com/ https://cdnjs.cloudflare.com/; object-src 'self'"
}